{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/(tabs)/signup.js",
    "app/(tabs)/signin.js",
    "app/(tabs)/chat.js",
    "app/(tabs)/home.js"
  ]
}
